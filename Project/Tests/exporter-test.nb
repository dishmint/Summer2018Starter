        RowBox[{
         RowBox[{"Transpose", "[", "passages", "]"}], "\[LeftDoubleBracket]", 
         "1", "\[RightDoubleBracket]"}], " ", "\[Rule]", " ", 
        RowBox[{"StringReplace", "[", 
