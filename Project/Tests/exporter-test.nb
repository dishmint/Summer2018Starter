       RowBox[{"absoluteFilePath", ",", " ", "\"\<String\>\""}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"xmlObject", " ", "=", " ", 
      RowBox[{"parseXML", "[", "fileAsString", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rootNode", " ", "=", " ", 
      RowBox[{"Cases", "[", 
