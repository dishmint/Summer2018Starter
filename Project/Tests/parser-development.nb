 RowBox[{
  RowBox[{"twineParser", "[", "absoluteFilePath_String", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "fileAsString", ",", "xmlObject", ",", " ", "rootNode", ",", 
      "rootNodeName", ",", " ", "passages", ",", "passageContentThread", ",", 
      "passageAssociation", ",", "edges", ",", "alledges", ",", " ", "g", ",",
       " ", "v"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Import", " ", "Twine", " ", "Project", " ", "as", " ", "string"}], 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"fileAsString", " ", "=", " ", 
      RowBox[{"Import", "[", 
       RowBox[{"absoluteFilePath", ",", " ", "\"\<String\>\""}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Explicitly", " ", "Parse", " ", "XML"}], " ", "|", " ", 
       RowBox[{
        RowBox[{"Riccardo", "'"}], "s", " ", "Algorithm"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"xmlObject", " ", "=", " ", 
      RowBox[{"parseXML", "[", "fileAsString", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Get", " ", "the", " ", "root", " ", "node"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"rootNode", " ", "=", " ", 
      RowBox[{"Cases", "[", 
       RowBox[{"xmlObject", ",", " ", 
        RowBox[{"XMLElement", "[", 
         RowBox[{"\"\<tw-storydata\>\"", ",", " ", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Rule", "[", 
             RowBox[{"\"\<name\>\"", ",", " ", "name_"}], "]"}], ",", "___"}],
            "}"}], ",", " ", "_"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Extract", " ", "the", " ", "Story", " ", "name"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"rootNodeName", " ", "=", 
      RowBox[{
       RowBox[{"Cases", "[", 
        RowBox[{"xmlObject", ",", " ", 
         RowBox[{
          RowBox[{"XMLElement", "[", 
           RowBox[{"\"\<tw-storydata\>\"", ",", " ", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"Rule", "[", 
               RowBox[{"\"\<name\>\"", ",", " ", "name_"}], "]"}], ",", 
              "___"}], "}"}], ",", " ", "_"}], "]"}], "\[RuleDelayed]", " ", 
          "name"}]}], "]"}], "/.", 
       RowBox[{
        RowBox[{"{", "storytitle_", "}"}], "\[Rule]", "storytitle"}]}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Extract", " ", "the", " ", "passages", " ", "in", " ", "the", " ", 
       "form", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"passage", "-", "name"}], ",", " ", "content"}], "}"}]}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"passages", " ", "=", " ", 
      RowBox[{"Cases", "[", 
       RowBox[{"rootNode", ",", " ", 
        RowBox[{
         RowBox[{"XMLElement", "[", 
          RowBox[{"\"\<tw-passagedata\>\"", ",", " ", 
           RowBox[{"{", 
            RowBox[{"___", ",", 
             RowBox[{"Rule", "[", 
              RowBox[{"\"\<name\>\"", ",", " ", "name_"}], "]"}], ",", 
             "___"}], "}"}], ",", " ", "content_"}], "]"}], "\[RuleDelayed]", 
         " ", 
         RowBox[{"{", 
          RowBox[{"name", ",", "content"}], "}"}]}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"0", ",", " ", "Infinity"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Create", " ", "Assocation", " ", "between", " ", "passage", " ", "and",
        " ", "content", " ", "for", " ", "tooltips"}], "*)"}], 
     "\[IndentingNewLine]", " ", 
     RowBox[{"passageContentThread", "=", " ", 
      RowBox[{"AssociationThread", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Transpose", "[", "passages", "]"}], "\[LeftDoubleBracket]", 
         "1", "\[RightDoubleBracket]"}], " ", "\[Rule]", " ", 
        RowBox[{
         RowBox[{"Transpose", "[", "passages", "]"}], "\[LeftDoubleBracket]", 
         "2", "\[RightDoubleBracket]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Add", " ", "rootnode", " ", "pair", " ", "to", " ", "association"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"passageAssociation", " ", "=", 
      RowBox[{"AssociateTo", "[", 
       RowBox[{"passageContentThread", ",", " ", 
        RowBox[{"rootNodeName", " ", "\[Rule]", " ", "\"\<Start Here\>\""}]}],
        "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Extract", " ", "Links"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"links", "=", " ", 
      RowBox[{"DeleteCases", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"extractLinks", "[", 
           RowBox[{
           "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], "]"}],
           "&"}], "/@", "passages"}], ",", " ", 
        RowBox[{"{", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"generate", " ", "edges"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"edges", " ", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"generatedEdges", "[", 
         RowBox[{
          RowBox[{"#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}],
           ",", " ", 
          RowBox[{"extractLinks", "[", 
           RowBox[{
           "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
           "]"}]}], "]"}], "&"}], "/@", "passages"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "create", " ", "complete", " ", "list", " ", "of", " ", "edges", " ", 
       "for", " ", "graph"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"alledges", "=", " ", 
      RowBox[{"Flatten", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"rootNodeName", " ", "\[DirectedEdge]", " ", 
           RowBox[{
            RowBox[{"First", "[", "passages", "]"}], "\[LeftDoubleBracket]", 
            "1", "\[RightDoubleBracket]"}]}], "}"}], ",", 
         RowBox[{"DeleteCases", "[", 
          RowBox[{"edges", ",", " ", 
           RowBox[{"{", "}"}]}], "]"}]}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Generate", " ", "Graph"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"g", " ", "=", 
      RowBox[{"Graph", "[", "alledges", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"v", " ", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Tooltip", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"VertexList", "[", "g", "]"}], "\[LeftDoubleBracket]", "i",
            "\[RightDoubleBracket]"}], ",", " ", 
          RowBox[{"passageAssociation", "[", 
           RowBox[{
            RowBox[{"VertexList", "[", "g", "]"}], "\[LeftDoubleBracket]", 
            "i", "\[RightDoubleBracket]"}], "]"}]}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", " ", "1", ",", " ", 
          RowBox[{"Length", "[", 
           RowBox[{"VertexList", "[", "g", "]"}], "]"}]}], "}"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"completeGraph", " ", "=", " ", 
      RowBox[{"Graph", "[", 
       RowBox[{"v", ",", " ", "alledges", ",", " ", 
        RowBox[{"VertexLabels", "\[Rule]", "\"\<Name\>\""}], ",", 
        RowBox[{"GraphLayout", " ", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"\"\<LayeredDigraphEmbedding\>\"", ",", " ", 
           RowBox[{"\"\<RootVertex\>\"", "\[Rule]", "rootNodeName"}]}], 
          "}"}]}]}], "]"}]}]}]}], "\[IndentingNewLine]", 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"test", "=", 
